[push]
	default = current
[alias]
	st = status
	ci = commit
	co = checkout
	br = branch
	last = log -1
	unstage = reset HEAD --
	ca = commit --amend
	tree=log --all --graph --decorate --oneline
    mv = "!branchmv() { git branch -m $1 $2; if [[ `git ls-remote --heads origin $1 | wc -l` -eq 1 ]]; then git push origin :$1; git push origin $2; fi }; branchmv"
    clear = "!stupid(){clear; echo 'Good job.'}; stupid"
	yeet = !git add . && git commit
	yolo = !git add . && git commit -m \"$(curl -s http://whatthecommit.com/index.txt)\"  && git push origin HEAD -f
[core]
	execludesfile = ~/.gitignore
	autocrlf = input
	pager = less -FMRiX
	editor = nvim
[fetch]
	prune = true
