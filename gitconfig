[push]
	default = current
[alias]
	st = status
	ci = commit
	co = checkout
	br = branch
	last = log -1
	unstage = reset HEAD --
	ca = commit --amend
[core]
	execludesfile = ~/.gitignore
	autocrlf = input
	pager = less -FMRiX
	editor = nvim
[commit]
	template = ~/.gitmessage
[fetch]
	prune = true
